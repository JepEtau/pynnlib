{
    "files.exclude": {
        "**/__pycache__": true,
        "**/dist": true,
        "**/*.egg-info": true,

        "**/*.pt": true,
        "**/*.pth": true,
        "**/*.safetensors": true,
        "**/*.onnx": true,
        "**/*.engine": true,
        "**/*.ncnn": true,
        "**/*.bin": true,
        "**/*.param": true,

        "**/*.png": true,
        "**/*.jpg": true,
        "**/*.mkv": true,
        "**/*.mp4": true,
        "**/*.mov": true,
        "**/*.mxf": true,
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": false,
    "files.trimTrailingWhitespace": true,
    "[python]": {
        "editor.rulers": [
            90,
            120
        ]
    },
    "python.analysis.extraPaths": [
        "../hytils"
    ],
}
